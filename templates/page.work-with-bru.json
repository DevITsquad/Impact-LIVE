    "ef60d6a0-4e1e-409c-90de-f06c2062bf8b": {
      "type": "images-with-text-scrolling",
      "blocks": {
        "template--16023833936033__ef60d6a0-4e1e-409c-90de-f06c2062bf8b-167705948148ed789f-0": {
          "type": "item",
          "settings": {
            "image": "shopify:\/\/shop_images\/running_silhouette_dropshipping_program_8cd4ad5e-a79e-4d86-956a-6014ab357ec8.jpg",
            "icon": "none",
            "icon_width": 48,
            "title": "Erster Schritt",
            "content": "<p><strong>Online-Shop<\/strong>: Nehmen Sieden BRU Maker One als Produkt in Ihren Online-Shop auf. Wir liefern digitale Inhalte und bieten Unterstützung bei der Integration.<br\/><strong>Offline-Shop<\/strong>: Nach der Registrierung erhalten Sie einen BRU Maker One, den Sie als Vorführgerät in Ihrem Geschäft verwenden können.<\/p>"
          }
        },
        "template--16023833936033__ef60d6a0-4e1e-409c-90de-f06c2062bf8b-167705948148ed789f-1": {
          "type": "item",
          "settings": {
            "image": "shopify:\/\/shop_images\/shopping_cart_dropshipping_program_42b1afae-bc9a-46aa-96fb-d866f7cc04d0.jpg",
            "icon": "none",
            "icon_width": 48,
            "title": "Verkauf",
            "content": "<p>Seien Sie unter den Ersten, die den BRU Maker One zum Verkauf anbieten.<br\/>Bestellung und Bezahlung erfolgt direkt über BRU.<\/p>"
          }
        },
        "b34f2dd7-d84f-4041-9ed6-3f459a7feeda": {
          "type": "item",
          "settings": {
            "image": "shopify:\/\/shop_images\/delivery_truck_dropshipping_program.jpg",
            "icon": "none",
            "icon_width": 48,
            "title": "Lieferung",
            "content": "<p>Die Bestellungen werden direkt vom BRU-Lager an den Endkunden versendet.<br\/>Vergabe der Sendungsverfolgungsnummer und  Kundenbetreuung erfolgt durch BRU.<\/p>"
          }
        },
        "4071dd51-b869-4d8d-af5b-75ce179846c4": {
          "type": "item",
          "settings": {
            "image": "shopify:\/\/shop_images\/piggy_bank_dropshipping_program.jpg",
            "icon": "none",
            "icon_width": 48,
            "title": "Auszahlung",
            "content": "<p>Am Ende eines jeden Monats wird die vorher abgemachte Provision pro verkaufte Maschine ausbezahlt.<\/p>"
          }
        }
      },
