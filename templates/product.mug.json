    "bacec0a0-0699-4921-bdf1-9363b510623d": {
      "type": "multi-column",
      "blocks": {
        "template--16142968062113__bacec0a0-0699-4921-bdf1-9363b510623d-item-1": {
          "type": "item",
          "settings": {
            "column_size": "3",
            "title": "spülmaschien geeignet",
            "heading_tag": "h4",
            "content": "<h5>Die Wasserfiltereinstellungen können auf die Härte Ihres Wassers eingestellt werden. <\/h5>",
